int S0 = 34;
int S1 = 35;
int S2 = 36;
int S3 = 37;
int OUTpin = 38;
int redcolor = 0;
int bluecolor = 0;
int greencolor = 0;
int redFrequency = 0;
int blueFrequency = 0;
int greenFrequency = 0;
void gecolor() {
  digitalWrite(S2, LOW);
  digitalWrite(S3, LOW);
  redFrequency = pulseIn(OUTpin, LOW);
  digitalWrite(S2, HIGH);
  digitalWrite(S3, HIGH);
  greenFrequency = pulseIn(OUTpin, LOW);
  digitalWrite(S2, LOW);
  digitalWrite(S3, HIGH);
  blueFrequency = pulseIn(OUTpin, LOW);
}
void setup() {
  pinMode(S0, OUTPUT);
  pinMode(S1, OUTPUT);
  pinMode(S2, OUTPUT);
  pinMode(S3, OUTPUT);
  pinMode(OUTpin, INPUT);
  Serial.begin(9600);
}

void loop() {
  
}
